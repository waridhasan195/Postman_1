		{
			"name": "POST Create User Creation",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.environment.set(\"email\", \"eve.holt@reqres.in\");\r",
							"pm.environment.set(\"password\", \"pistol\")\r",
							"\r",
							"\r",
							"\r",
							"let email = pm.variables.get(\"email\")\r",
							"console.log(\"Email: \"+email)\r",
							"\r",
							"let pass = pm.variables.get(\"password\");\r",
							"console.log(\"Password: \" + pass)\r",
							"\r",
							"\r",
							"let commonURL = pm.globals.get(\"commonURL\")\r",
							"console.log(\"Common Url: \"+ commonURL)\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200, Your Request Successfull\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    console.log(\"Overall Success\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Status code is 400, Your Request is BAD\", function () {\r",
							"    pm.response.to.have.status(400);\r",
							"    console.log(\"Failed\");\r",
							"});\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
