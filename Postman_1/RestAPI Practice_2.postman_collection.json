								"exec": [
									"\r",
									"pm.test(\"Body is correct EMAIL\", function () {\r",
									"    pm.response.to.have.body(\"email\");\r",
									"});\r",
									"\r",
									"pm.test(\"Body is correct PASSWORD\", function () {\r",
